{
  "type": "recurring",
  "identifier": "demographics",
  "title": "Demographics",
  "guid": "demographics",
  "activity": {
    "type"      : "elementList",
    "identifier": "demographics_list",
    "elements"   : [
      {
        "identifier": "treatStart",
        "type": "singleChoiceText",
        "text": "Have you started any treatments since [survey date]?",
        "items": [
          {
            "prompt": "No",
            "value": "0"
          },
          {
            "prompt": "Yes",
            "value": "1"
          }
        ]
      },
      {
        "identifier": "treatChange",
        "type": "singleChoiceText",
        "text": "Have there been any changes in your treatments since [survey date]?",
        "items": [
          {
            "prompt": "No",
            "value": "0"
          },
          {
            "prompt": "Yes",
            "value": "1"
          }
        ]
      },
      {
        "identifier": "treatEnd",
        "type": "singleChoiceText",
        "text": "Have you stopped any treatments since [survey date]?",
        "items": [
          {
            "prompt": "No",
            "value": "0"
          },
          {
            "prompt": "Yes",
            "value": "1"
          }
        ]
      },
      //      {
      //        "identifier": "age",
      //        "type": "numericInteger",
      //        "text": "What is your age?",
      //        "range": {
      //          "min": 18,
      //          "max": 99,
      //          "unitLabel": "years"
      //        }
      //      },
      {
        "identifier": "zip_code",
        "type": "textfield",
        "text": "What is your zip code?",
        "validationRegex": "^[0-9]{5}$",
        "invalidMessage": "Please enter a valid zip code."
      }
    ]
  },
  "resultTransforms":[
    {
      "transform":"SurveyResult",
      "inputMapping":[
        {
          "stepIdentifier":"treatStart",
          "parameter":"treatStart"
        },
        {
          "stepIdentifier":"treatChange",
          "parameter":"treatChange"
        },
        {
          "stepIdentifier":"treatEnd",
          "parameter":"treatEnd"
        },
        {
          "stepIdentifier":"zip_code",
          "parameter":"zip_code"
        },
        {
          "parameter": "schemaID",
          "constant": {
            "namespace": "Cornell",
            "name": "rs-survey",
            "version": "1.0.0"
          }
        }
      ]
    }
  ]

}



